#!/bin/sh
xrandr --output DVI-D-0 --mode 1920x1080 --pos 0x768 --rotate normal --output HDMI-0 --off --output DP-0 --primary --refresh 144 --mode 1920x1080 --pos 1920x768 --rotate normal --output DP-1 --off --output HDMI-1 --mode 1360x768 --pos 1920x0 --rotate normal --output DP-2 --off --output DP-3 --off
